[Unit]
Description=Running jnlp to connect slaves to jenkins
After=network.target

[Service]
Type=simple
User=jenkins
Group=jenkins
LimitNOFILE=<%= @my_nofiles %>
ExecStart=/usr/bin/java -jar /home/jenkins/slave.jar -jnlpUrl <%= @jnlpurl %> -secret <%= @secret_key %>

[Install]
WantedBy=multi-user.target

